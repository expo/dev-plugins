{
  "name": "@bycedric/inspect-test",
  "version": "0.1.0",
  "description": "Inspect the bundle stats from Metro",
  "keywords": [
    "expo",
    "devtools"
  ],
  "bin": "build/src/cli/bin.js",
  "main": "build/src/index.js",
  "types": "build/src/index.d.ts",
  "files": [
    "build",
    "dist",
    "expo-module.config.json",
    "metro.js",
    "metro.d.ts",
    "webui/dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "expo-module build",
    "build:all": "expo-module prepare && cd webui && npx expo export -p web --output-dir ../dist",
    "clean": "expo-module clean",
    "web:dev": "cd webui && npx expo start -w",
    "web:export": "cd webui && npx expo export -p web --output-dir ../dist"
  },
  "license": "MIT",
  "dependencies": {
    "@expo/server": "^0.3.1",
    "arg": "^5.0.2",
    "compression": "^1.7.4",
    "expo-router": "~3.4.7",
    "express": "^4.18.2",
    "freeport-async": "^2.0.0",
    "getenv": "^1.0.0",
    "morgan": "^1.10.0",
    "open": "^8.4.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "expo": "~50.0.1",
    "expo-module-scripts": "^3.1.0",
    "typescript": "^5.1.3"
  },
  "peerDependencies": {
    "expo": "*"
  }
}
